{
    "name": "cnviradiya/laravel-filepond",
    "description": "Laravel module for the filepond uploads",
    "license": "MIT",
    "keywords": [
        "laravel",
        "php",
        "filepond",
        "upload",
        "image",
        "drag-drop-file"
    ],
    "authors": [{
        "name": "Chirag Viradiya",
        "email": "viradiyachirag91@gmail.com"
    }],
    "require": {
        "php": "^8.0",
        "illuminate/support": "5.5.*|5.6.*|5.7.*|5.8.*|^6.0|^7.0|^8.0",
        "illuminate/contracts": "5.5.*|5.6.*|5.7.*|5.8.*|^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "cnviradiya\\LaravelFilepond\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "cnviradiya\\LaravelFilepond\\LaravelFilepondServiceProvider"
            ]
        }
    },
    "minimum-stability": "1.0.0",
    "prefer-stable": true
}